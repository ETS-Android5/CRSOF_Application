<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.CRSOF_Application">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Hide"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.CRSOF_Application.usebutton"></activity>
        <activity android:name="com.example.android.CRSOF_Application.Decoder" />
        <activity android:name="com.example.android.CRSOF_Application.Encoder" />
        <activity android:name="com.example.android.CRSOF_Application.encode" />
        <activity android:name="com.example.android.CRSOF_Application.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>